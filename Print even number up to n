import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
     Scanner scan=new Scanner(System.in);
        int i,n;
        n=scan.nextInt();
        
        for(i=2;i<=n;i++)
        {
            if(i%2==0){
                System.out.print(i+" ");
            }
        }
    }
}
