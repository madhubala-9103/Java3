import java.util.Scanner;

public class Solution {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int N = sc.nextInt();
        int oddCount = 0;
        for (int i = 0; i < N; i++) {
            int num = sc.nextInt();
            if (num % 2 != 0) { 
                oddCount++;
            }
        }
        System.out.println(oddCount);
        
    }
}
