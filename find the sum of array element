import java.util.Scanner;

public class Solution {
    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
        int N= scan.nextInt();
        int sum = 0;
        int[] arr=new int[N];
        for(int i=0;i<N;i++){
            arr[i]=scan.nextInt();
            sum+=arr[i];
        } System.out.println(sum);
    }
}

        
