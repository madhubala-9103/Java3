import java.io.*;
import java.util.*;
public class Solution {    
    public static void main(String[] args) { 
        Scanner scan=new Scanner (System.in);   
        String a=scan.next();     
        int b=scan.nextInt(); 
        int c=scan.nextInt(); 
        StringBuilder sb=new StringBuilder(a); 
        sb.delete(b,c);        
        System.out.println(sb);   
    }
}
